wscript.echo "#!ipxe"if WScript.Arguments.count>0 then wscript.echo "echo " & WScript.Arguments.Item(0)wscript.echo "set boot-url http://${dhcp-server}"wscript.echo "kernel ${boot-url}/wimboot"wscript.echo "initrd ${boot-url}/bootmgr                     bootmgr"wscript.echo "initrd ${boot-url}/boot/BCD                    BCD"wscript.echo "initrd ${boot-url}/Boot/boot.sdi               boot.sdi"wscript.echo "initrd ${boot-url}/sources/boot.wim            boot.wim"wscript.echo "boot"